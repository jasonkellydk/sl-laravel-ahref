{
    "name": "jasonkellydk/sl-laravel-ahref",
    "type": "library",
    // TODO add description
    "description": "UNDER DEVELOPMENT",
    "keywords": [
        "JasonKellyDK",
        "SlLaravelAhref"
    ],
    "homepage": "https://github.com/JasonKellyDK/SlLaravelAhref",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "JasonKellyDK\\SlLaravelAhref\\": "src/SpotOnLive/Ahref/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JasonKellyDK\\SlLaravelAhref\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
